{
  "name": "react-tailwind-starter",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.0.4",
    "@testing-library/user-event": "^12.1.7",
    "axios": "^0.20.0",
    "postcss": "^8.1.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-dropzone": "^11.2.0",
    "react-hook-form": "^6.9.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^3.4.3",
    "tailwindcss": "^1.8.10"
  },
  "scripts": {
    "purge:css": "cross-env NODE_ENV=production postcss src/tailwind.css -o src/index.css",
    "build:css": "postcss src/tailwind.css -o src/index.css",
    "watch:css": "postcss src/tailwind.css -o src/index.css -w",
    "start": "npm run build:css && react-scripts start",
    "build": "npm run purge:css && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^10.0.1",
    "cross-env": "^7.0.2",
    "cssnano": "^4.1.10",
    "postcss-cli": "^8.0.0"
  }
}
